<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>sign</servlet-name>
        <servlet-class>com.google.appengine.demos.guestbook.SignGuestbookServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>trainmodel</servlet-name>
        <servlet-class>com.google.appengine.demos.guestbook.TrainModelServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>checkmodel</servlet-name>
        <servlet-class>com.google.appengine.demos.guestbook.CheckModelServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>sign</servlet-name>
        <url-pattern>/sign</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>checkmodel</servlet-name>
        <url-pattern>/checkmodel</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>trainmodel</servlet-name>
        <url-pattern>/trainmodel</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>guestbook.jsp</welcome-file>
    </welcome-file-list>
</web-app>
